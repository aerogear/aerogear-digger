 {
  "kind": "PersistentVolume",
  "apiVersion": "v1",
  "metadata": {
    "name": "pv50"
  },
  "spec": {
    "capacity": {
    "storage": "10Gi"
  },
  "accessModes": [
    "ReadWriteOnce"
  ],
  "persistentVolumeReclaimPolicy": "Recycle",
  "nfs": {
    "path": "/tmp/jenkins/android-sdk-linux",
    "server": "0.0.0.0"
  }
}
