{
  "kind": "PersistentVolume",
  "apiVersion": "v1",
  "metadata": {
    "name": "android-sdk-pv"
  },
  "spec": {
    "capacity": {
      "storage": "10Gi"
    },
    "accessModes": [
      "ReadWriteOnce"
    ],
    "persistentVolumeReclaimPolicy": "Recycle",
    "hostPath": {
      "path": "/tmp/jenkins/android-sdk-linux"
    }
  }
}
